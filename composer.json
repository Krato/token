{
    "name": "infinety-es/token",
    "description": "Custom token generation",
    "keywords": ["laravel 5", "token", "infinety"],
    "license": "MIT",
    "authors": [
        {
            "name": "Eric Lagarda",
            "email": "info@ericlagarda.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/support": "5.1.* || 5.2.*",
        "vinkla/hashids": "^2.2"
    },
    "require-dev": {
        "graham-campbell/testbench": "^3.1",
        "phpunit/phpunit": "^4.8 || ^5.0"
    },
    "autoload": {
        "psr-4": {
            "Infinety\\Token\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
